---
import Layout from "@layouts/landingPage";
import Stores from '@parts/stores'

const data = {
	meta: {
		title: "Contact Us - Innovative Eye Care",
	},
};
---

<style lang="scss">
	h1 {
		margin: 4em 0 1em;
		text-align: center;
	}

	@media (min-width: 40em) {
		h1 {
			margin: 5em 0 0;
		}
	}

	@media (min-width: 60em) {
		h1 {
			font-size: 2.5em;
			margin: 4em 0 0;
		}
	}
</style>

<Layout content={data}>
	<h1>Contact</h1>
	<Stores />
</Layout>

